{
  "jsc": {
    "parser": {
      "syntax": "ecmascript",
      "decorators": true,
      "decoratorsBeforeExport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "target": "es5",
    "externalHelpers": false
  },
  "module": {
    "type": "commonjs"
  },
  "sourceMaps": true
}
